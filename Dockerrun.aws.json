{
    "AWSEBDockerrunVersion": 3,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "soroush123/multi-client-10-14",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "soroush123/multi-server-10-14",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "soroush123/multi-worker-10-14",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "soroush123/multi-nginx-10-14",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "Links": ["client", "server"],
            "memory": 128
        }
    ]
}
